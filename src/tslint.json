{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "bc",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "bc",
            "kebab-case"
        ],
        "one-line": false,
        "no-unused-variable": false,
        "max-line-length": false,
        "no-unused-expression": false,
        "no-trailing-whitespace": false
    }
}
